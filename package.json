{
  "name": "react-tutorial",
  "version": "2.0.16",
  "description": "Tutorial for React",
  "repository": {
    "type": "git",
    "url": "https://github.com/svenanders/react-tutorial"
  },
  "main": "public/src/scripts/app.js",
  "scripts": {
    "start": "webpack -p && node start.js",
    "test": "jest",
    "postinstall": "gulp build && gulp critical",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot --content-base build --config webpack.config.dev.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testFileExtensions": [
      "es6",
      "js"
    ],
    "moduleFileExtensions": [
      "js",
      "json",
      "es6"
    ],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/underscore",
      "<rootDir>/node_modules/jquery",
      "<rootDir>/node_modules/reflux",
      "tty",
      "net"
    ],
    "globals": {
      "__DEV__": true
    }
  },
  "config": {
    "blanket": {
      "pattern": "public/src/scripts",
      "data-cover-never": [
        "dist",
        "node_modules",
        "__tests__/",
        "tests"
      ],
      "loader": "../../../tests/blanket-stub-jsx.js"
    }
  },
  "keywords": [
    "react",
    "reflux",
    "boilerplate"
  ],
  "engines": {
    "node": "0.10.x",
    "npm": "2.1.x"
  },
  "author": "Sven Anders Robbestad",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^4.4.5",
    "babel-jest": "^4.0.0",
    "babel-loader": "^4.0.0",
    "body-parser": "^1.10.0",
    "bootstrap": "^3.3.1",
    "bootstrap-sass": "^3.3.1",
    "c3": "^0.4.x",
    "compression": "^1.3.0",
    "critical": "^0.5.5",
    "d3": "^3.5.x",
    "debug": "^2.1.2",
    "es5-shim": "^4.0.5",
    "del": "^1.1.1",
    "express": "^4.10.6",
    "extend": "^2.0.0",
    "firebase": "^2.2.0",
    "glob": "^4.4.0",
    "jshint-stylish-ex": "^0.2.0",
    "jquery": "^2.1.3",
    "jsx-loader": "^0.12.2",
    "loader-utils": "^0.2.6",
    "lodash": "^3.3.1",
    "marked": "^0.3.2",
    "mcfly": "0.0.10",
    "mime": "^1.3.4",
    "morgan": "^1.5.1",
    "node-jsx": "^0.12.4",
    "object-assign": "^2.0.0",
    "react": "^0.12.x",
    "react-bootstrap": "^0.15.x",
    "react-breadcrumbs": "^0.2.x",
    "react-hammerjs": "^0.2.0",
    "react-headroom": "^1.3.2",
    "react-interpolate-component": "^0.6.2",
    "react-markdown-to-html": "^1.0.6",
    "react-router": "^0.12.x",
    "react-router-bootstrap": "^0.9.x",
    "react-tools": "^0.12.2",
    "reactfire": "^0.4.0",
    "reactify": "^0.17.1",
    "reflux": "^0.2.7",
    "rx": "^2.4.3",
    "serve-favicon": "^2.2.0",
    "superagent": "^0.21.0",
    "superagent-promise": "^0.2.0",
    "underscore": "^1.7.0",
    "validator": "^3.33.0",
    "webpack": "^1.7.0",
    "gulp": "^3.8.10",
    "gulp-sass": "^1.2.4",
    "gulp-webpack": "^1.2.0",
    "gulp-jshint": "^1.9.4",
    "gulp-load-plugins": "^0.8.1",
    "gulp-concat": "^2.4.2",
    "gulp-jsx": "^0.8.0",
    "gulp-autoprefixer": "^2.0.0",
    "gulp-babel": "^4.0.0",
    "gulp-cssmin": "^0.1.6",
    "gulp-rename": "^1.2.0",
    "gulp-shell": "^0.2.11",
    "gulp-sourcemaps": "^1.3.0",
    "gulp-uglify": "^1.0.2",
    "gulp-util": "^3.0.1",
    "gulp-watch": "^3.0.0",
    "gulp-react": "^3.0.1"  
  },
  "devDependencies": {
    "babel": "^4.4.5",
    "flux": "^2.0.1",
    "jsdom": "^4.0.0",
    "vinyl-buffer": "^1.0.0",
    "watchify": "^2.2.1",
    "browserify": "^7.0.3",
    "mocha-lcov-reporter": "0.0.1",
    "jest": "^0.1.37",
    "jest-cli": "^0.2.1",
    "mocha": "^2.1.0",
    "react-hot-loader": "^1.1.1",
    "react-stickydiv": "^3.3.0",
    "webpack-dev-server": "^1.7.0"
  }
}
